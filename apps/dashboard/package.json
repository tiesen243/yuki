{
  "name": "@yuki/dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun with-env react-router build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "bun with-env react-router dev --port 3001",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "react-router typegen && eslint",
    "start": "bun with-env react-router-serve ./build/server/index.js --port 3001",
    "typecheck": "react-router typegen && tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@yuki/prettier-config",
  "dependencies": {
    "@react-router/fs-routes": "^7.2.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "@t3-oss/env-core": "^0.12.0",
    "@tanstack/react-query": "^5.66.7",
    "@trpc/client": "^11.0.0-rc.795",
    "@trpc/server": "^11.0.0-rc.795",
    "@trpc/tanstack-react-query": "^11.0.0-rc.795",
    "@yuki/api": "workspace:*",
    "@yuki/auth": "workspace:*",
    "@yuki/ui": "workspace:*",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0",
    "superjson": "^2.2.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.2.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vercel/react-router": "^1.0.2",
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "dotenv-cli": "^8.0.0",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
