{
  "name": "@yuki/dashboard",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun with-env vue-tsc -b && vite build",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bun with-env vite --port 3001",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "bun with-env vite preview --port 3001",
    "typecheck": "vue-tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@yuki/prettier-config",
  "dependencies": {
    "@t3-oss/env-core": "^0.12.0",
    "@tanstack/vue-query": "^5.65.0",
    "@trpc/client": "^11.0.0-rc.730",
    "@vueuse/integrations": "^12.5.0",
    "@yuki/api": "workspace:*",
    "@yuki/db": "workspace:*",
    "@yuki/ui": "workspace:*",
    "superjson": "^2.2.2",
    "universal-cookie": "^7.2.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "dotenv-cli": "^8.0.0",
    "tailwindcss": "^4.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vue-tsc": "^2.2.0"
  }
}
