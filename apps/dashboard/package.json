{
  "name": "@yuki/dasboard",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun with-env nuxt build",
    "clean": "git clean -xdf .cache .nuxt .output .turbo node_modules",
    "dev": "bun with-env nuxt dev --port 3001",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "eslint",
    "start": "bun with-env nuxt preview --port 3001",
    "typecheck": "nuxt typecheck",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@yuki/prettier-config",
  "dependencies": {
    "@nuxt/fonts": "^0.10.3",
    "@vueuse/core": "^12.5.0",
    "@yuki/ui": "workspace:*",
    "nuxt": "^3.15.4",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.0.0",
    "@tailwindcss/vite": "^4.0.3",
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.7.3"
  }
}
