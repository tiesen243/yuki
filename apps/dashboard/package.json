    "universal-cookie": "^7.2.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "eslint": "^9.20.0",
    "prettier": "^3.5.0",
